      \@tfor\@c:=#2\do{%
        \if\@c L%
            \h@ko左外true%
        \else\if\@c ^%
            \h@ko上付true%
        \else\if\@c l%
            \h@ko左寄true%
        \else\if\@c |%
            \h@ko縦仕切true%
