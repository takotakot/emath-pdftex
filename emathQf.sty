        \ifthenelse{\equal{\kai@type}{kyosuu}}{%
          \Cfor{\edef\Qf@D{1}}{\lengthtest{\Qf@D\p@>\z@}\or\lengthtest{\Qf@D\p@=\z@}}\do{%
