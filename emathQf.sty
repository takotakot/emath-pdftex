              \else
                \prH\prQ@Ltmp \leqq \mitis@@ \leqq \prH\prQ@Rtmp
              \fi
            }{%
            }%
          }%
        }%
      }%
    \else\ifnum\prQ@D=\z@
      \ifthenelse{\equal{\hutou@gou}{>}}{%
        \prHsort[s]\prQ@Ltmp~以外のすべての実数
      }{%
        \ifthenelse{\equal{\hutou@gou}{>=}}{%
          すべての実数
        }{%
          \ifthenelse{\equal{\hutou@gou}{<}}{%
            解なし
          }{%
            \ifthenelse{\equal{\hutou@gou}{<=}}{%
              \prHsort[s]\prQ@Ltmp
            }{%
            }%
          }%
        }%
      }%
    \else
      \ifthenelse{\equal{\hutou@gou}{>}}{%
        すべての実数
      }{%
        \ifthenelse{\equal{\hutou@gou}{>=}}{%
          すべての実数
        }{%
          \ifthenelse{\equal{\hutou@gou}{<}}{%
            解なし
          }{%
            \ifthenelse{\equal{\hutou@gou}{<=}}{%
              解なし
            }{%
            }%
          }%
        }%
      }%
    \fi\fi
  }%
}%
