\def\@cut#1{%
  \edef\@cut@{\empty}%
  \expandafter\@for\expandafter\@cut@@c\expandafter:\expandafter=#1\do{%
    \ifx\empty\@cut@
      \ifnum\@cut@@c=\z@\else
        \edefappend\@cut@{\@cut@@c}%
      \fi
    \else
      \edefappend\@cut@{,\@cut@@c}%
    \fi
  }%
  \ifx\empty\@cut@\edef#1{0}\else\edef#1{\@cut@}\fi
}%
