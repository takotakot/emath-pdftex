    \Ransuu[d]{Int(25*X/\ran@sup-12)}\iQ@i
    \Ransuu[d]{Int(25*X/\ran@sup-12)}\iQ@ii
    \IAdd\iQ@i\iQ@ii\Qf@b\relax
