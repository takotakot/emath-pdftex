            \Ransuu[d]{Int(((\aii@@max)-(\aii@@min)+1)*X/\ran@sup+(\aii@@min))}\Qf@a
            \ifnum\Qf@a=\z@
          \repeat
          \Ransuu[d]{Int(((\ai@@max)-(\ai@@min)+1)*X/\ran@sup+(\ai@@min))}\Qf@b
          \Ransuu[d]{Int(((\a@@max)-(\a@@min)+1)*X/\ran@sup+(\a@@min))}\Qf@c
