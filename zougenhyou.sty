    \@ifundefined{zh@outotu}{}{%
      \csvhairetu*{\zh@TTL}{zh@ttl}%
      \EMedef\zh@ddyname{\zh@ttliii}%
      \EMedef\zh@yname{\zh@ttliv}%
    }%
