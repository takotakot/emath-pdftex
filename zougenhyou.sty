  \@ifundefined{zh@outotu}{}{%
    \ifodd\kyokuti@xN@ddy
      \ifnum\inv@ddy=\z@\def\ddy@sign{-}\else\def\ddy@sign{+}\fi
    \else
      \ifnum\inv@ddy=\z@\def\ddy@sign{+}\else\def\ddy@sign{-}\fi
    \fi
