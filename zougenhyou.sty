      \def\zh@outotu{}%
      \edefhairetu{zh@dysign}{\zh@i}{0}%
      \edefhairetu{zh@ddysign}{\zh@i}{1}%
      \Decr\kyokuti@xN@dy
    \else
      \Decr\kyokuti@xN@ddy
      \edefhairetu{zh@dysign}{\zh@i}{1}%
      \edefhairetu{zh@ddysign}{\zh@i}{0}%
    \fi\fi
    \EMedefappend\zh@xstr{&\zh@tmp&\cdots}%
  }%
