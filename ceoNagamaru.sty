\let\nagamaru\@undefined
\gdef\ceoNagamaru#1{%
  \isnumber{#1}{%
    \edef\ceo@tmp{#1}%
    \ifnum\ceo@tmp>9\errmessage{ceoNagamaru:counter too large:#1}\fi
    \ifnum\ceo@tmp=0 \edef\ceo@tmp{58}%
    \else \IAddself\ceo@tmp{48}%
    \fi
  }{%
    \Strchr\ALPHlist{#1}\ceo@tmp
    \ifnum\ceo@tmp=\z@\errmessage{ceoNagamaru:#1 is not defined.}\fi
    \IAddself\ceo@tmp{64}%
  }%
  \mbox{%
    \fontencoding{U}\fontfamily{nmaru}\fontseries{m}\fontshape{n}\selectfont
      \char\ceo@tmp
  }%
}%
%\let\nagamaru\ceoNagamaru
\gdef\ceobNagamaru#1{%
  \isnumber{#1}{%
    \edef\ceo@tmp{#1}%
    \ifnum\ceo@tmp>9\errmessage{ceoNagamaru:counter too large:#1}\fi
    \ifnum\ceo@tmp=0 \edef\ceo@tmp{42}%
    \else \IAddself\ceo@tmp{32}%
    \fi
  }{%
    \Strchr\ALPHlist{#1}\ceo@tmp
    \ifnum\ceo@tmp=\z@\errmessage{ceoNagamaru:#1 is not defined.}\fi
    \IAddself\ceo@tmp{96}%
  }%
  \mbox{%
    \fontencoding{U}\fontfamily{nmaru}\fontseries{m}\fontshape{n}\selectfont
      \char\ceo@tmp
  }%
}%
%\let\bnagamaru\ceobNagamaru
