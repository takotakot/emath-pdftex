    my $xo=\TS@x;
    my $yo=\TS@y;
    my $zo=\TS@z;
    my $l=\TS@xx-$xo;
    my $m=\TS@yy-$yo;
    my $n=\TS@zz-$zo;
    my $t=-($a*$xo+$b*$yo+$c*$zo+$d)/($a*$l+$b*$m+$c*$n);
      printf FHNDL "(\EMpercentchar s,\EMpercentchar s,\EMpercentchar s)",
