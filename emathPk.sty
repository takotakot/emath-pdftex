\def\perlPandL#1#2#3#4#5#6{%
% \typeout{PandL:arg1=#1,arg2=#2,arg3=#3,arg4=#4,arg5=#5}%
  \vecXYZ{#1}\TS@xi\TS@yi\TS@zi
  \vecXYZ{#2}\TS@xii\TS@yii\TS@zii
