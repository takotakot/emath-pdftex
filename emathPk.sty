\def\iiiperlKyori#1#2#3{%
% #1: 点P
% #2,#3,#4: 平面上の点A, B, C
% #5: 垂線の足（戻り値）
  \vecXYZ{#1}\TS@xi\TS@yi\TS@zi
  \vecXYZ{#2}\TS@xii\TS@yii\TS@zii
  \CalcVal{%
    my $vxi=\TS@xi;
    my $vyi=\TS@yi;
    my $vzi=\TS@zi;
    my $vxii=\TS@xii;
    my $vyii=\TS@yii;
    my $vzii=\TS@zii;
       printf FHNDL "\EMpercentchar s\string\n",
        sqrt(($vxi-$vxii)**2+($vyi-$vyii)**2+($vzi-$vzii)**2);}#3\relax
  }%
