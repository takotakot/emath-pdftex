    my $xo=\Sandl@xo;
    my $yo=\Sandl@yo;
    my $zo=\Sandl@zo;
    my $xi=\Sandl@xi;
    my $yi=\Sandl@yi;
    my $zi=\Sandl@zi;
    my $l=\Sandl@l;
    my $m=\Sandl@m;
