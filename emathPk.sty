\def\perlpSuisen#1#2#3#4{%
% #1: 点P
% #2: 平面上の点A
% #3: 平面の法線ベクトル(nvec)
% #4: 垂線の足（戻り値）
  \vecXYZ{#1}\pS@Px\pS@Py\pS@Pz
  \vecXYZ{#2}\pS@Ax\pS@Ay\pS@Az
  \vecXYZ{#3}\pS@vx\pS@vy\pS@vz
  \CalcVal{%
    my $ax=\pS@Ax;
    my $ay=\pS@Ay;
    my $az=\pS@Az;
    my $px=\pS@Px;
    my $py=\pS@Py;
    my $pz=\pS@Pz;
    my $vx=\pS@vx;
    my $vy=\pS@vy;
    my $vz=\pS@vz;
       $ax-=$px;
