\def\YoungTableau{\@ifnextchar[{\@YoungTableau}{\@YoungTableau[\empty]}}%
\def\@YoungTableau[#1]#2{{%
  \csvhairetu*[;]{#2}{YT@row}%
  \edef\YT@tmp{\hairetu{YT@row}{1}}%
  \csvhairetu*{\YT@tmp}{YT@top}%
  \edef\YT@xmax{\YT@topN}%
  \edef\YT@ymax{\YT@rowN}%
  \edef\YT@y{\YT@ymax}%
  \edef\YT@tbl{}%
  \Ifor*\YT@i{1}{\YT@rowN}\Do{%
    \edef\YT@tmp{\hairetu{YT@row}{\YT@i}}%
    \csvhairetu*{\YT@tmp}{YT@iln}%
    \EMedefappend\YT@tbl{\hairetu{YT@iln}{1}}%
    \Ifor*\YT@j{2}\YT@ilnN\Do{%
     \edefappend\YT@tbl{&\hairetu{YT@iln}{\YT@j}}%
    }%
    \EMedefappend\YT@tbl{\noexpand\\ \noexpand\cline{1-\YT@ilnN}}%
  }%
  \begin{tabular}'cellsize=\Young@Dcellsize'{|*\YT@xmax{C{-}|}}\hline
    \YT@tbl
  \end{tabular}
