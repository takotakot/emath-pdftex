\def\YoungDiagram{\@ifnextchar[{\@YoungDiagram}{\@YoungDiagram[\empty]}}%
\def\@YoungDiagram[#1]#2{{%
  \csvhairetu*{#2}{YD@hairetu}%
  \edef\YD@xmax{\hairetu{YD@hairetu}{1}}%
  \Ifor*\YD@i{2}{\YD@hairetuN}\Do{%
    \edef\YD@tmp{\hairetu{YD@hairetu}{\YD@i}}%
    \ifnum\YD@tmp>\YD@xmax\edef\YD@xmax{\YD@tmp}\fi
  }%
  \edef\YD@ymax{\YD@hairetuN}%
  \edef\YD@y{\YD@ymax}%
  \edef\YD@tbl{}%
  \Ifor*\YD@i{1}{\YD@hairetuN}\Do{%
    \edef\YD@jmax{\hairetu{YD@hairetu}{\YD@i}}%
    \Ifor*\YD@j{2}\YD@jmax\Do{%
     \EMedefappend\YD@tbl{&}%
    }%
    \ifnum\YD@i<\YD@hairetuN
      \IAdd\YD@i{1}\YD@ii
      \edef\YD@jjmax{\hairetu{YD@hairetu}{\YD@ii}}%
      \ifnum\YD@jjmax>\YD@jmax\edef\YD@jmax{\YD@jjmax}\fi
    \fi
    \EMedefappend\YD@tbl{\noexpand\\ \noexpand\cline{1-\YD@jmax}}%
  }%
  \edef\YD@tmp{\hairetu{YD@hairetu}{1}}%
  \begin{tabular}'cellsize=\Young@Dcellsize'[\Young@D@haiti]%
        {|*\YD@xmax{C{-}|}}\cline{1-\YD@tmp}%
    \YD@tbl
  \end{tabular}
