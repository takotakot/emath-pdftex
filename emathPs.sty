\def\fc@eps@pdf#1#2{%
% 1: eps ファイルがない
% 2: pdf ファイルがない
% 3: pdf ファイルが古い
  \CalcVal{%
  my $ret=-1;
  my $mtimeeps=0;
  my $mtimepdf=0;
  my $mtimexbb=0;
  ($mtimeeps) = (stat("#1.eps"))[9];
  if ($mtimeeps==""){$ret=1;}
  else {
    ($mtimepdf) = (stat("#1.pdf"))[9];
    ($mtimexbb) = (stat("#1.xbb"))[9];
    if ($mtimepdf==""){
      $ret=2;
    }
    elsif ($mtimepdf<$mtimeeps) {%
      $ret=3;
    }%
    if ($mtimexbb!="" && $mtimexbb<$mtimeeps) {
      unlink("#1.xbb");
    }
  }
  printf FHNDL "\EMpercentchar s\string\n", $ret;
  }#2}%
