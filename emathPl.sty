\def\Gseq#1#2#3#4{%
% #1: 初項
% #2: 公比
% #3: 項数
% #4: 結果を受け取る配列基幹名
  \hairetusyokika{#4}%
  \CalcVals{%
    my $syokou=#1;
    my $kouhi=#2;
    my $kousuu=#3;
    my $ans=$syokou;
    print FHNDL $ans, "\string\n";
    my $ival=1;
    while ($ival++<$kousuu) {
      $ans*=$kouhi;
      if (abs($ans)<0.0001) {
        printf FHNDL "\@percent .8f\string\n",$ans;
      } else {
        print FHNDL $ans, "\string\n";
      }
    }
  }{#4}%
}%
