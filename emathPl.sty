\def\calcval@{\@ifnextchar[{\@calcval}{\@calcval[f]}}%
\def\@calcval[#1]#2#3{%\begingroup
%\typeout{calcval:arg=#2}%
%  \edef\by@perl{0}%
  \check@perlp@ss
  \ifcase\perlp@ss
    \@perljob@sub
    \ifnum\skip@perl=\z@
      \immediate\write\em@whndl{open(FHNDL,">\perl@datafilename");}%
\@requirePerlLib
