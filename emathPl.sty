    }else{
      $ansi=(-$b-sqrt($D))/(2*$a);if (abs($ansi)<\emLlim){$ansi=0;}
      $ansii=(-$b+sqrt($D))/(2*$a);if (abs($ansii)<\emLlim){$ansii=0;}
      printf FHNDL "\EMpercentchar s,\EMpercentchar s\string\n",$ansi,$ansii;
    }
  }\perlEqii@ans
