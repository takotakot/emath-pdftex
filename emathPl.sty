\def\calcval{%
  \@ifundefined{not@perl@check}{%
    \IfFileExists{checkperl.sty}{%
      \let\calcval\calcval@
      \calcval@
    }{%
      \errmessage{emathPl Error : Add --shell-escape option for latex}%
    }%
  }{\calcval@}%
