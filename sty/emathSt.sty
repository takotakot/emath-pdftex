  \edef\EMst@min{\empty}%
  \edef\EMst@kosuu{\empty}%
  \edef\EMst@souwa{\empty}%
  \edef\EMst@heikin{\empty}%
  \edef\EMst@bunsan{\empty}%
  \edef\EMst@hyouzyunhensa{\empty}%
