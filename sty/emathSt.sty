  \edef\EMst@lhinge{\empty}%
  \edef\EMst@uhinge{\empty}%
  \edef\EMst@sibunni{\empty}%
  \edef\EMst@sibunniii{\empty}%
  \edef\EMst@sorted{\empty}%
  \edef\EMst@rsorted{\empty}%
