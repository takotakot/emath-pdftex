\def\goukei#1#2{%
  \CalcVal{%
    my @dta=(#1);
    my $SUM=0;
    foreach (@dta){
      $SUM+=$_;
    }
    printf FHNDL"\@percent s\string\n",$SUM;
  }#2\relax
}%
\def\heikin#1#2{%
  \CalcVal{%
    my @dta=(#1);
    my $SUM=0;
    my $n=0;
    foreach (@dta){
      $SUM+=$_;
      $n++;
    }
    printf FHNDL"\@percent s\string\n",$SUM/$n;
  }#2\relax
}%
\def\bunsan#1#2{%
  \CalcVal{%
    my @dta=(#1);
    my $SUM=0;
    my $SSUM=0;
    my $n=0;
    foreach (@dta){
      $SUM+=$_;
      $SSUM+=$_*$_;
      $n++;
    }
    printf FHNDL"\@percent s\string\n",$SSUM/$n-($SUM/$n)**2;
  }#2\relax
}%
\def\hyouzyunhensa#1#2{%
  \CalcVal{%
    my @dta=(#1);
    my $SUM=0;
    my $SSUM=0;
    my $n=0;
    foreach (@dta){
      $SUM+=$_;
      $SSUM+=$_*$_;
      $n++;
    }
    printf FHNDL"\@percent s\string\n",sqrt($SSUM/$n-($SUM/$n)**2);
  }#2\relax
}%
