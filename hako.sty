  \@@@@maskHako(#1)[#2][#3]<\empty>}}%
\def\@@@@maskHako(#1)[#2][#3]<#4>#5{{\fboxsep=\z@
  \ifx\empty #1\relax
      \hako{\vrule height #2 depth #3 width\z@
          \ifmaskhako
            {\everymaskHako@b{\ensuremath{#4\phantom{#5}}}%
                \everymaskHako@e}%
          \else
            {\everymaskHako@b{\ensuremath{#5}}\everymaskHako@e}%
          \fi
      }%
  \else
      \hako{\vrule height #2 depth #3 width\z@
          \ifmaskhako
            \makebox[#1][l]{\everymaskHako@b{\ensuremath{#4}}\everymaskHako@e}%
          \else
            \makebox[#1][c]{\everymaskHako@b{\ensuremath{#5}}\everymaskHako@e}%
          \fi
      }%
  \fi
}}%
